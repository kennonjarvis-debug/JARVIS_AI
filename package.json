{
  "name": "jarvis-control-plane",
  "version": "2.0.0",
  "description": "Jarvis - Central Control Plane for AI Dawg",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "dev:gateway": "tsx watch src/core/gateway.ts",
    "build": "tsc",
    "start": "node dist/main.js",
    "test": "jest",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "jest --config tests/e2e/jest.config.js --runInBand",
    "test:alerts": "tsx scripts/test-alerts.ts",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "audit:deadcode": "tsx scripts/audit-deadcode.ts",
    "audit:size": "node scripts/audit-size.mjs",
    "audit:all": "npm run audit:size && npm run audit:deadcode",
    "imessage": "tsx src/agents/imessage/cli.ts",
    "imessage:start": "tsx src/agents/imessage/cli.ts start",
    "imessage:status": "tsx src/agents/imessage/cli.ts status"
  },
  "keywords": [
    "ai",
    "jarvis",
    "control-plane",
    "orchestration"
  ],
  "author": "Jarvis Team",
  "license": "MIT",
